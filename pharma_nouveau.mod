dvar float x1;
dvar float x2;
dvar float x3;
dvar float x4;

maximize
  6.5*x1 + 11.5*x2 + 6*x3 + 10*x4;
  
subject to {
  2.5*x1 + 7.5*x2 <= 2404;
  2.5*x3 + 7.5*x4 <= 250;
  0.125*x1 + 0.125*x2 + 0.125*x3 + 0.125*x4 <= 51;
  17.5*x1 + 10*x2 + 17.5*x3 + 10*x4 <= 5950;
  x1 >= 0;
  x2 >= 0;
  x3 >= 0;
  x4 >= 0;
}