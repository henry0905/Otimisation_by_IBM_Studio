dvar float x1;
dvar float x2;

maximize
  6.5*x1 + 11.5*x2;
  
subject to {
  2.5*x1 + 7.5*x2 <= 2404;
  0.125*x1 + 0.125*x2 <= 51;
  17.5*x1 + 10*x2 <= 5950;
  x1 >= 0;
  x2 >= 0;
}